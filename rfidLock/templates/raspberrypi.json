{
  "role": "client",
  "rfid_interface": "/dev/ttyAMA0",
  "baud": 9600,
  "timeout": 3.0,
  "lock_pin": 7,
  "unlock_pin": 8,
  "start_tx_pin": 17,
  "lock_button_pin": 23,
  "resource": "{{ resource }}",
  "table_name": "{{ table_name }}",
  "local_database": "{{ sqlite }}",
  "client_log": "/var/log/rfid_client.log",
  "database": {
    "user": "{{ mysql[0] }}",
    "password": "{{ mysql[1] }}",
    "database": "{{ mysql[2] }}",
    "host": "{{ mysql[3] }}"
  }
}
